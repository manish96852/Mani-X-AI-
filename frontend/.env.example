# Environment Configuration Example
# Copy this file to .env and update the values for your local development

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080
REACT_APP_API_TIMEOUT=10000

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_REACT_QUERY_DEVTOOLS=true
REACT_APP_ENABLE_API_LOGGING=true

# Hedera Network Configuration (integrates with existing networks.ts)
# Valid values: "testnet" | "mainnet"
REACT_APP_HEDERA_NETWORK=testnet

# Other Configuration
REACT_APP_APP_NAME=MANI X AI
REACT_APP_VERSION=0.1.0
