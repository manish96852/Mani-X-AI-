# Environment Configuration for Production
# Copy this file to .env.production for production builds

# API Configuration
REACT_APP_API_BASE_URL=https://api.manixai.com
REACT_APP_API_TIMEOUT=15000

# Environment
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_REACT_QUERY_DEVTOOLS=false
REACT_APP_ENABLE_API_LOGGING=false

# Hedera Network Configuration
REACT_APP_HEDERA_NETWORK=mainnet
REACT_APP_HEDERA_MIRROR_NODE=https://mainnet.mirrornode.hedera.com

# Other Configuration
REACT_APP_APP_NAME=MANI X AI
REACT_APP_VERSION=1.0.0
