    "NETWORK": "testnet",
    "HEDERA_ACCOUNT_ID": "0.0.6515212",
    "GEMINI_API_KEY": "AIzaSyBspJ6rJRUJACeT3rMUEhQgbSeRyj-HV5o"
  },
  "functions": {
    "api/index.rs": {
      "runtime": "vercel-rust@4.1.0"
    }
  }
}
